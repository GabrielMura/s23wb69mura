extends layout

block content
  h1= title
  -let image_names = ["drawn-tree.png", "leafy-tree.jpg", "maple-tree.jpg", "potted-tree.jpg", "sonora-tree.jpg"];
  -for(let i = 0; i < 3; i++) 
    -let randnum = Math.floor(Math.random()*5);
    img(src= `\images\\${image_names[randnum]}`, alt="trees", height=350, width=350)
  